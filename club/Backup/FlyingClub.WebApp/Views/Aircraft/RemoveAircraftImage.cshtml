@{
    ViewBag.Title = "Remove Image";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Remove Image</h2>
<h3>Are you sure you want to remove image @ViewBag.imageName of @ViewBag.tailNumber?</h3>
<table>
<tr>
<td>@Html.ActionLink("Remove", "RemoveImage", new { imageId = ViewBag.imageId})</td>
<td>@Html.ActionLink("Cancel", "Edit", new { aircraftId = ViewBag.aircraftId })</td>
</tr>
</table>
